{
  "name": "web-nextjs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node --experimental-modules server.mjs",
    "test": "cross-env NODE_ENV=test jest",
    "jest-update": "cross-env NODE_ENV=test jest --updateSnapshot",
    "dev": "npm-run-all --parallel fake-api-gateway dev-server",
    "dev-server": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 GITHUB_AUTH_URL=\"/authentication?issuer=github&code=123\" node --experimental-modules server.mjs",
    "build": "next build",
    "export": "next export",
    "analyze": "cross-env ANALYZE=1 next build",
    "fake-api-gateway": "node --experimental-modules ./fake/api-gateway/server.mjs"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.1.6",
    "express": "^4.16.3",
    "isomorphic-unfetch": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "next": "^7.0.1",
    "next-redux-saga": "^3.0.0",
    "next-redux-wrapper": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^22.4.4",
    "json-server": "^0.14.0",
    "sinon": "^5.1.0",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
